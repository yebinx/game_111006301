{
  "compilerOptions": {
    "module": "commonjs",
    "lib": [
      "es2015",
      "es2017",
      "dom",
      "ES2020.BigInt"
    ],
    "target": "es5",
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "outDir": "temp/vscode-dist",
    "forceConsistentCasingInFileNames": true
  },
  "exclude": [
    "node_modules",
    "library",
    "local",
    "temp",
    "build",
    "settings"
  ]
}